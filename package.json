{
  "name": "react-webpack-boilerplate",
  "version": "1.1.0",
  "description": "Internal project for discover new features of webpack and react stack",
  "repository": {
    "type": "git",
    "url": "https://github.com/zmeecer/react-webpack-boilerplate.git"
  },
  "author": "zmeecer zmeecer@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/prod.js",
    "start": "webpack-dev-server --config ./webpack/dev.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "test": "jest",
    "flow": "flow"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-core": "^6.22.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.41.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "storyshots": "^3.2.2",
    "webpack": "^2.2.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.4.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass|svg)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.jsx?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
